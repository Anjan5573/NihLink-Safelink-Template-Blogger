                <b:includable id='aboutPostAuthor'/><b:includable id='addComments'/><b:includable id='commentAuthorAvatar'/><b:includable id='commentDeleteIcon'/><b:includable id='commentForm'/><b:includable id='commentFormIframeSrc'/><b:includable id='commentItem'/><b:includable id='commentList'/><b:includable id='commentPicker'/><b:includable id='comments'/><b:includable id='commentsTitle'/><b:includable id='feedLinks'/><b:includable id='feedLinksBody'/><b:includable id='homePageLink'/><b:includable id='iframeComments'/><b:includable id='inlineAd'/><b:includable id='nextPageLink'/><b:includable id='post'/><b:includable id='postBody'/><b:includable id='postBodySnippet'/><b:includable id='postCommentsAndAd'/><b:includable id='postCommentsLink'/><b:includable id='postFooter'/><b:includable id='postFooterAuthorProfile'/><b:includable id='postHeader'/><b:includable id='postJumpLink'/><b:includable id='postMeta'/><b:includable id='postPagination'/><b:includable id='postTitle'/><b:includable id='previousPageLink'/><b:includable id='status-message'/><b:includable id='threadedCommentForm'/><b:includable id='threadedCommentJs'/><b:includable id='threadedComments'/>
                <b:includable id='main' var='this'>
                  <b:comment>var='this' digunakan untuk mengirim setiap data allBylineItems agar bisa terbaca</b:comment>

             	    <b:loop values='data:posts' var='post'>
            		    <b:include name='article.loop'/>
            	    </b:loop>

                </b:includable>

                <b:includable id='article.loop'>
                  <b:if cond='data:view.isMultipleItems'>

                    <article id='content-card' expr:class='&quot;content index post-&quot; + data:post.id + &quot; blog&quot;' itemscope='itemscope' itemtype="https://schema.org/BlogPosting">
            	         <b:include name='article.loop.index'/>
                     </article>

                  <b:else/>

                    <main class="content post" aria-label="Main content" itemscope='itemscope' itemtype="https://schema.org/Blog">
            	         <article expr:class='&quot;post-&quot; + data:post.id + &quot; blog&quot;' itemscope='itemscope' itemprop="blogPost" itemtype="https://schema.org/BlogPosting">
            		           <b:include name='article.loop.post'/>
            	         </article>
                    </main>
                  </b:if>
                </b:includable>


            <b:includable id='article.loop.index'>

            <div class='thumbnail'>
            <img class='trans' b:whitespace='remove'>
            <b:attr expr:value='data:post.title ? data:post.title : data:messages.image' name='alt' />
            <b:attr expr:value='data:post.title ? data:post.title : data:messages.image' name='title' />
            <b:if cond='data:post.featuredImage'>
            <b:attr expr:value='resizeImage(data:post.featuredImage, 240)' name='src'/>
            <b:else/>
            <b:attr value='https://placehold.it/750x440/4a138c/fff?text=NO+THUMBNAIL' name='src' />
            </b:if>
            </img>
            </div>
            <a expr:href='data:post.link ? data:post.link : data:post.url' expr:title='data:post.title ? data:post.title : data:messages.noTitle' class='btn'></a>
            <h3>
            	<a expr:href='data:post.link ? data:post.link : data:post.url' expr:title='data:post.title ? data:post.title : data:messages.noTitle'>
            		<b:eval expr='data:post.title ? data:post.title snippet { length: 5, links: false, linebreaks: false, ellipsis: true } : data:messages.noTitle' />
            	</a>
            </h3>
            <div class='deskripsi'><b:eval expr='data:post.snippets.long snippet { length: 100, links: false, linebreaks: false, ellipsis: true }'/></div>

            </b:includable>

            <b:includable id='article.loop.post'>

            <div class='post-body' itemprop="articleBody">
              <h3 class="text-center"><data:post.title/></h3>
            	<data:post.body/>
            </div>

            </b:includable>

            <b:includable id='article.pagination'>
            <b:if cond='not data:view.isSingleItem'>
            <b:if cond='data:olderPageUrl'>
            <div class='col-sm-12 col-xs-12'>
            <div  id='linkloadmore'>
            	<a expr:href='data:olderPageUrl' expr:id='data:widget.instanceId + &quot;_pagination-old&quot;' expr:title='data:messages.olderPosts' itemprop='url'>
            		<span itemprop='name'>Next Page</span>
            	</a>
            </div>
            </div>
            </b:if>
            </b:if>
            </b:includable>

            </b:widget>
            </b:section>

            <b:section id='reset'/>

          </div>


          <div class="col-12 col-md-3 order-last mb-3" id="side-right">
            <b:section class='ads-right' cond='!data:view.isPreview' id='ads-right' maxwidgets='1'>
          		<b:widget id='HTML704' locked='true' title='right-sidebar-ads' type='HTML' version='2' visible='true'>
            		<b:widget-settings>
              		<b:widget-setting name='content'><![CDATA[<img src="//dummyimage.com/700x700/ccc/777.png&text=+++ADS+++" alt="text=ads" title="ads" class="d-block w-100 mb-3" /><img src="//dummyimage.com/700x700/ccc/777.png&text=+++ADS+++" alt="text=ads" title="ads" class="d-block w-100 mb-3" />]]></b:widget-setting>
            		</b:widget-settings>
            		<b:includable id='main'>
          				<b:if cond='data:view.isSingleItem'>
          					<data:content/>
          				</b:if>
          			</b:includable>
          		</b:widget>
          	</b:section>
          </div>

        </div>

      </div>
    </div>
  </div>
</div>

